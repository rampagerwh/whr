    <selectionEntry id="b870-eb5d-c408-b554" name="Zakar Rustdox y su equipo infernal" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
