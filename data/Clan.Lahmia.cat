            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="a077-c79e-af0c-f03c" type="max"/>
          </constraints>
          <profiles>
            <profile id="0a75-b2f7-73ad-2bfc" name="Dificil de matar" hidden="false" typeId="9680-f788-862f-a30d" typeName="Poder vampírico">
              <characteristics>
                <characteristic name="Propiedades" typeId="b0e0-e0cf-2cd1-3ec3">La vampira es inmune a Golpe letal y Golpe letal heroico.</characteristic>
