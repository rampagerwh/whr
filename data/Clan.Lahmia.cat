        </selectionEntry>
        <selectionEntry id="a7b7-3235-ff1b-276e" name="Velocidad mercúrea" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="d77f-2f28-35b1-8853" type="max"/>
          </constraints>
          <profiles>
            <profile id="b1c1-add0-04ec-3257" name="Velocidad mercúrea" hidden="false" typeId="9680-f788-862f-a30d" typeName="Poder vampírico">
              <characteristics>
                <characteristic name="Propiedades" typeId="b0e0-e0cf-2cd1-3ec3">Toda unidad que reciba una carga de la vampira o su unidad solo puede aguantar la posición como reacción a la carga.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="25.0"/>
