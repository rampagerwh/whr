          <profiles>
            <profile id="7c0c-20d0-03cf-4efb" name="Maestra arcana" hidden="false" typeId="9680-f788-862f-a30d" typeName="Poder vampírico">
              <characteristics>
                <characteristic name="Propiedades" typeId="b0e0-e0cf-2cd1-3ec3">La vampira gana un nivel de magia, hasta un máximo de 4.</characteristic>
