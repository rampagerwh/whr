    <selectionEntry id="1709-9ebf-39db-fd03" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="bdee-8a88-ee15-0b19" type="equalTo"/>
