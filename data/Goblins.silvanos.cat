    <selectionEntry id="7aef-770d-a047-88a4" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="3032-0970-763b-52e3" type="equalTo"/>
