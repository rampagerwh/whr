        </rule>
        <rule id="9a97-f9bf-f10d-b614" name="Señor del conocimiento (saberes de Tzeentch, Magia oscura, Fuego, Sombras, Muerte, Metal, Luz, Cielos, Bestias y Vida)" hidden="false"/>
