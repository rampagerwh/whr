            <selectionEntry id="40af-7aa7-18d1-a296" name="Dos cabezas" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="52f8-2980-f85f-9045" type="max"/>
