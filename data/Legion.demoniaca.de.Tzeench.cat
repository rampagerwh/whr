            <selectionEntry id="7eb5-4248-7616-b277" name="Dos cabezas" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1019-fddd-e7a8-7c5a" type="max"/>
