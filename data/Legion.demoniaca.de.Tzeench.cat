            <selectionEntry id="10b3-858f-2deb-820c" name="Dos cabezas" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="00d1-6d61-af6a-c9b0" type="max"/>
