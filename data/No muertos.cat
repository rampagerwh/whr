      <rules>
        <rule id="32f6-87df-93fa-0c67" name="Magia Nigromante" hidden="false">
          <description>Nivel de magia 1, puede aumentarse a 2 por +35 puntos. Puede elegir hechizos de Nigromancia, Magia Oscura, del Saber de la Muerte y/o del Saber de las Sombras.</description>
        </rule>
        <rule id="15f0-62bf-3d82-7709" name="¡Está vivo! Nigromante" hidden="false">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
              <conditions>
                <condition field="selections" scope="03eb-6034-750b-9243" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="7766-8913-1c29-7bd6" type="equalTo"/>
