        <rule id="3f10-5071-ac19-05fd" name="Magia Maximilian Schreiber" hidden="false">
          <description>Max es un hechicero de nivel 3 que siempre utiliza hechizos del Saber del Metal.</description>
        </rule>
        <rule id="59e2-8598-6470-2f8d" name="Señor del conocimiento (Metal)" hidden="false"/>
