        <selectionEntry id="b6fa-ec68-ed91-ac95" name="Pócima gélida ungol" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="d31d-8dfb-74a8-e479" type="max"/>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="4748-f490-fab0-98e1" type="max"/>
