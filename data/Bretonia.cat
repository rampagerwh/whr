    <selectionEntry id="fa45-c029-4b20-880d" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="ee2b-5030-8547-ed41" type="equalTo"/>
          </conditions>
