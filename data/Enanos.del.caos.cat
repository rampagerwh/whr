    <profile id="5468-2df8-09f1-4ceb" name="Bombas de nafta" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Arma">
      <characteristics>
        <characteristic name="Rango" typeId="52616e676523232344415441232323">6&quot;</characteristic>
