    <selectionEntry id="f3ed-7ea1-cbeb-6595" name="Cañón infernal" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="b59c-aa8c-9d11-5123" value="2.0">
          <conditions>
