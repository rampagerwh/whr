    <selectionEntry id="781c-6f49-ad3f-5772" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="224c-49cb-b622-c7f8" type="equalTo"/>
