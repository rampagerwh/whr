    <selectionEntry id="c382-4735-4e28-c746" name="Leviatán de tierra" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="eb52-09fd-5ce2-f06d" value="2.0">
          <conditions>
