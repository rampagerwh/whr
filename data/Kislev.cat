    <selectionEntry id="a5fb-77cb-672e-e0e2" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="2638-ffb7-8bfe-bdf9" type="equalTo"/>
