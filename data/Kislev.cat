        <rule id="44c5-d1a2-84dc-209f" name="Magia Maximilian Schreiber" hidden="false">
          <description>Max es un hechicero de nivel 3 que siempre utiliza hechizos del Saber del Metal.</description>
        </rule>
        <rule id="c13e-21b0-7748-6c61" name="Señor del conocimiento (Metal)" hidden="false"/>
