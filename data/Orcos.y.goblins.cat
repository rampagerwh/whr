            <selectionEntry id="4093-cdae-3d95-0cb6" name="Mázkara zinieztra" hidden="false" collective="false" import="true" type="upgrade">
              <constraints>
                <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="227d-5f0f-acff-b450" type="max"/>
