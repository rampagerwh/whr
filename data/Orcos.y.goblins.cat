    <selectionEntry id="4ac6-a8a9-8f85-1775" name="Fanático" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="fd89-0a96-c1a5-51e0" name="Fanático" hidden="false" typeId="4d6f64656c23232344415441232323" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="4d23232344415441232323">2D6</characteristic>
