    <profile id="60d7-3056-1ab4-2137" name="Esfera de viento corrosivo" hidden="false" typeId="576561706f6e23232344415441232323" typeName="Arma">
      <characteristics>
        <characteristic name="Rango" typeId="52616e676523232344415441232323">12&quot;</characteristic>
        <characteristic name="Fuerza" typeId="537472656e67746823232344415441232323">-</characteristic>
        <characteristic name="Reglas especiales" typeId="5370656369616c2052756c657323232344415441232323">Arrojadiza, DIsparo rápido, Ataques mágicos, Causan heridas con 4+ sin salvación por armadura, si obtiene un 1, habrá estallado antes de tiempo y el lanzador se hiere a sí mismo.</characteristic>
      </characteristics>
