    <selectionEntry id="d614-a498-f942-d278" name="Bestia del cieno" hidden="false" collective="false" import="true" type="upgrade">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
            <condition field="selections" scope="roster" value="0.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" childId="1618-967c-fd85-0392" type="equalTo"/>
