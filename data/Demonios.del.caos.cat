    <selectionEntry id="6f56-0b62-ad67-0733" name="Piro-Incinerador de Tzeentch" hidden="false" collective="false" import="true" type="unit">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="fd6e-be2b-7eec-b0b9" type="max"/>
