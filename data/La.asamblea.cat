    <selectionEntry id="d0b5-3d42-2045-e107" name="Tanque sopero &quot;Kathleen&quot;" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="1196-a1b3-e2ba-214a" type="max"/>
