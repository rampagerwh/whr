      </constraints>
      <selectionEntries>
        <selectionEntry id="1b13-ec61-9bc6-9883" name="Kaldero de nieblaz" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="false" includeChildSelections="false" includeChildForces="false" id="a6fc-edbe-822b-4b6b" type="max"/>
