    <selectionEntry id="94e8-9933-f34b-1d75" name="Paztorez de garrapatoz goblins nocturnos" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="66f4-f3a4-2c64-5175" value="3.0">
