    <selectionEntry id="31c0-2c7a-bf88-d2c5" name="Gigante" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="f6c5-7c0d-7fa2-c684" value="2.0">
          <conditions>
