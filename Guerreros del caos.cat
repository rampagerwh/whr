    <selectionEntry id="6090-3376-ffe7-bcf0" name="Galrauch, el gran draco, primero de los dragones del caos" hidden="false" collective="false" import="true" type="unit">
      <modifiers>
        <modifier type="set" field="hidden" value="true">
          <conditions>
